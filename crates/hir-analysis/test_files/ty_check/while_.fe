pub fn factorial(mut num: u32) -> u32 {
    let mut res = 1
    while num > 0 {
        res *= num
        num -= 1
    }

    res
}

pub fn factorial2(mut num: u32) -> u32 {
    let mut res = 1
    while true {
        res *= num
        if num > 1 {
            num -= 1
        } else {
            return num
        }
    }

    res
}
