pub trait Logger {
    fn log(self) -> u8
}

pub struct Console {}

impl Logger for Console {
    fn log(self) -> u8 {
        1
    }
}

pub fn log_once() -> u8
  uses (logger: Logger)
{
    logger.log()
}

pub fn provide_logger() -> u8 {
    let logger = Console {}
    with (Logger = logger) {
        log_once()
    }
}
