pub struct StructWithGenericParam<S, T, U> {
    x: S
    y: T
    z: U
}
 
pub struct StructWithGenericParam2<
    S,
    T: foo::Trait,
    U
> {
    x: *(S, *i32)
    y: T
    z: U
}

pub struct StructWithGenericParam3<
    S: foo::Trait + bar::Trait,
    T,
    U: bar::Trait
> {
    x: S
    y: T
    z: U
}