impl Trait<T> for F<T> {
    fn foo() {
        return 1
    }
}

impl Trait<T, U> for F<T> 
where T: Clone
      U: Bar
{
    fn foo<T: OtherTrait<U>>(t: T) {
        do_something<i32>(t)
    }
}

impl Trait<T: Clone, U: Bar> for F<U: Bar> 
{
    fn foo<T: OtherTrait<U>>(t: T) {
        do_something<i32>(t)
    }
}