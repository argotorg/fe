fn loop_examples(arr: Array<i32, 10>) {
    for i in arr {
        if i == 5 { continue }
        if i > 8 { break }
    }

    let mut x = 0
    while x < 100 {
        x = x + 1
        if x % 2 == 0 { continue }
    }

    let mut maybe = Some(1)
    while let Some(v) = maybe {
        maybe = None
        if v > 0 { break }
    }
}
