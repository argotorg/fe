// For loop over an array with #[unroll] attribute
// The loop body will be emitted multiple times instead of using a loop
pub fn for_array_unrolled_sum() -> u64 {
    let arr: [u64; 5] = [1, 2, 3, 4, 5]
    let mut sum: u64 = 0
    #[unroll]
    for elem in arr {
        sum = sum + elem
    }
    sum
}
