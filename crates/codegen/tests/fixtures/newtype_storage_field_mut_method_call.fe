msg Msg {
    #[selector = 1]
    Bump -> u256
}

struct Wrap {
    inner: u256,
}

impl Wrap {
    fn bump(self) -> Self {
        Wrap { inner: self.inner + 1 }
    }
}

pub contract NewtypeStorageFieldMutMethodCall {
    pad: u256
    w: Wrap

    init() {}

    recv Msg {
        Bump -> u256 uses (mut w) {
            w = w.bump()
            w.inner
        }
    }
}
