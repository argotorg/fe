// Borrow handles returned from calls should point at the call in diagnostics.

struct Pair {
    a: u256,
    b: u256,
}

fn borrow_a(x: mut Pair) -> mut u256 {
    mut x.a
}

pub fn borrowck_call_result_then_write() {
    let mut s: Pair = Pair { a: 0, b: 0 }
    let p: mut u256 = borrow_a(mut s)
    s.a = 1
    p = 2
}
