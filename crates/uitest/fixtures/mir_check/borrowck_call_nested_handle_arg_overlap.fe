struct HandleBox {
    h: mut u256,
}

fn take2(a: mut HandleBox, b: mut HandleBox) {
    a.h += 1
    b.h += 1
}

pub fn borrowck_call_nested_handle_arg_overlap() {
    let mut value: u256 = 0
    let handle: mut u256 = mut value
    let mut a: HandleBox = HandleBox { h: handle }
    let mut b: HandleBox = HandleBox { h: handle }
    take2(mut a, mut b)
}
