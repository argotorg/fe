// Minimal smoke test: bare recv block with a qualified msg variant.

msg BankMsg {
    #[selector = 0x01]
    Deposit { amount: u64 } -> u64,
}

pub contract BareRecvContract {
    data: u64,

    recv {
        BankMsg::Deposit { amount } -> u64 uses (mut data) {
            data += amount
            data
        }
    }
}
