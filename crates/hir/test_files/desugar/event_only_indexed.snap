use std::prelude::*
use std::evm::Address

struct Approval {
    owner: Address,
    spender: Address,
    value: u256,
}

impl std::evm::Event for Approval {
    const TOPIC0: u256 = core::keccak(("Approval", "(", Address::SOL_TYPE, ",", Address::SOL_TYPE, ",", u256::SOL_TYPE, ")"))

    fn emit<L: std::evm::Log>(self, log: mut L) {
        log.log4(0, 0, Self::TOPIC0, self.owner.as_topic(), self.spender.as_topic(), self.value.as_topic())
    }
}
