use core::prelude::*
use std::prelude::*
use std::evm::Address

pub struct Transfer {
    pub from: Address,
    pub to: Address,
    pub amount: u256,
}

impl std::evm::Event for Transfer {
    const TOPIC0: u256 = core::keccak(("Transfer", "(", Address::SOL_TYPE, ",", Address::SOL_TYPE, ",", u256::SOL_TYPE, ")"))

    fn emit<L: std::evm::Log>(self, log: mut L) {
        let mut enc = std::abi::SolEncoder::new()
        let data_len = std::abi::encoded_size<u256>()
        enc.reserve_head(data_len)
        self.amount.encode(mut enc)
        let (data_ptr, data_len) = enc.finish()
        log.log3(data_ptr, data_len, Self::TOPIC0, self.from.as_topic(), self.to.as_topic())
    }
}
