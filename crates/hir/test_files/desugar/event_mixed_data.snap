use core::prelude::*
use std::evm::Address

struct Mixed {
    from: Address,
    flag: bool,
    value: i128,
}

impl std::evm::Event for Mixed {
    const TOPIC0: u256 = core::keccak(("Mixed", "(", Address::SOL_TYPE, ",", bool::SOL_TYPE, ",", i128::SOL_TYPE, ")"))

    fn emit<L: std::evm::Log>(self, log: mut L) {
        let mut enc = std::abi::SolEncoder::new()
        let data_len = std::abi::encoded_size<(bool, i128)>()
        enc.reserve_head(data_len)
        (self.flag, self.value).encode(mut enc)
        let (data_ptr, data_len) = enc.finish()
        log.log2(data_ptr, data_len, Self::TOPIC0, self.from.as_topic())
    }
}
