use core::prelude::*

struct Flags {
    ok: bool,
    delta: i8,
    value: u256,
}

impl std::evm::Event for Flags {
    const TOPIC0: u256 = core::keccak(("Flags", "(", bool::SOL_TYPE, ",", i8::SOL_TYPE, ",", u256::SOL_TYPE, ")"))

    fn emit<L: std::evm::Log>(self, log: mut L) {
        let mut enc = std::abi::SolEncoder::new()
        let data_len = std::abi::encoded_size<u256>()
        enc.reserve_head(data_len)
        self.value.encode(enc)
        let (data_ptr, data_len) = enc.finish()
        log.log3(data_ptr, data_len, Self::TOPIC0, self.ok.as_topic(), self.delta.as_topic())
    }
}
