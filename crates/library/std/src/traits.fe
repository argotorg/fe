impl IsNumeric for u256 {}
impl IsNumeric for i8 {}
impl IsNumeric for i16 {}
impl IsNumeric for i32 {}
impl IsNumeric for i64 {}
impl IsNumeric for i128 {}
impl IsNumeric for i256 {}
