contract Foo {
    is_true: bool

    pub fn bar(mut self, ctx: Context) -> u256 {
        self.is_true = false
        return baz(ctx)
    }

    fn baz(ctx: Context) -> u256 {
        return ctx.block_number()
    }
}

#test
fn test_foo(mut ctx: Context) {
    let mut foo: Foo = Foo.create(ctx, 0)
    assert foo.bar(ctx) == 0
}